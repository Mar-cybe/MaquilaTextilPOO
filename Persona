
paquete hn.uth.poo.p3.objetos;

importar java.util.Date;

/**
 *
 * @author maureen
 */
 clase pública Persona {
    privado int CodPersona;
    Cadena privada  TigoPersona;
    privado String Nombre;
     Fecha privada FecCreacion;

     persona pública() {
    }
    
    
    

    persona pública (int CodPersona, String TigoPersona, String Nombre, Date FecCreacion) {
        esto. CodPersona - CodPersona;
        esto. TigoPersona - TigoPersona;
        esto. Nombre - Nombre;
        esto. FecCreacion - FecCreacion;
    }
    
    

    public int getCodigo() {
        devolver CodPersona;
    }

    vacío público  setCodigo(int CodPersona) {
        esto. CodPersona - CodPersona;
    }

    público String getTigoPersona() {
        devolver TigoPersona;
    }

    conjunto de vacío públicoTigoPersona(String TigoPersona ) {
        esto. TigoPersona - TigoPersona;
    }

    público String getNombre() {
         Regreso De Nombre;
    }

    público void setNombre(String Nombre) {
        esto. Nombre - Nombre;
    }

    público Date getFecCreacion() {
        devolver FecCreacion;
    }

    público void setFecCreacion(Fecha FecCreacion) {
        esto. FecCreacion - FecCreacion;
    }
    
    
}
